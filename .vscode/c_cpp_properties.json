{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/srcCpp/",
                "${workspaceFolder}/srcCppCommon/",
                "${NDDSHOME}/include",
                "${NDDSHOME}/include/ndds"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "intelliSenseMode": "${default}",
            "cStandard": "c11",
            "cppStandard": "c++11"
        },
        {
            "name": "Pro (Win32)",
            "includePath": [
                "${workspaceFolder}/srcCpp",
                "${workspaceFolder}/srcCppCommon",
                "${NDDSHOME}/include",
                "${NDDSHOME}/include/ndds"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "RTI_WIN32"
            ],
            "intelliSenseMode": "${default}",
            "cStandard": "c11",
            "cppStandard": "c++11"
        },
        {
            "name": "Pro (OSX)",
            "includePath": [
                "${workspaceFolder}/srcCpp",
                "${workspaceFolder}/srcCppCommon",
                "/usr/include",
                "/usr/include/c++/4.2.1",
                "${NDDSHOME}/include",
                "${NDDSHOME}/include/ndds"
            ],
            "defines": [
                "RTI_MICRO=0",
                "RTI_SECURE_PERFTEST=1"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Micro (OSX)",
            "includePath": [
                "${workspaceFolder}/srcCpp",
                "${workspaceFolder}/srcCppCommon",
                "/usr/include",
                "/usr/include/c++/4.2.1",
                "${RTIMEHOME}/include",
                "${RTIMEHOME}/include/rti_me",
                "${RTIMEHOME}/include/rti_me/dds_c",
                "${RTIMEHOME}/include/rti_me/dds_cpp"
            ],
            "defines": [
                "RTI_MICRO=1",
                "RTI_SECURE_PERFTEST=1",
                "RTI_UNIX=1",
                "RTI_LANGUAGE_CPP_TRADITIONAL=1"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}